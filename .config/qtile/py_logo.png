  <body data-page="Image Page">
    <span class="loaders">
      <div class="spinner"></div>
      <div class="progress">
        <div class="track">
          <div class="bar"></div>
        </div>
      </div>
      <div class="broken-link">Image not found</div>
    </span>
    <div class="container">
      <div class="image-container">
        <div class="gad gad-top">
            <div id="image-top" style="max-height: 200px;"></div>
            <script type="text/javascript">
            nads.createAd('image-top', {
              "refreshLimit": 10,
              "refreshTime": 30,
              "report": {
                "enabled": true,
                "wording": "Report Ad",
                "position": "fixed-bottom-right"
              }
            });
            </script>
        </div>
        <div class="image-inner-container">
          <div class="image-content">
            
            <img data-src="https://s3-eu-west-1.amazonaws.com/cdn.pbrd.co/images/KaaHPVn.png" data-load-src="https://pasteboard.co/images/KaaHPVn.png/load" alt="uploaded image" class="image js">
            <noscript> <img src="https://s3-eu-west-1.amazonaws.com/cdn.pbrd.co/images/KaaHPVn.png" alt="uploaded image" class="image"> </noscript>
            
          </div>
        </div>
        <div class="gad gad-bottom">
            <div id="image-bottom" style="max-height: 200px;"></div>
            <script type="text/javascript">
            nads.createAd('image-bottom', {
              "refreshLimit": 10,
              "refreshTime": 30,
              "report": {
                "enabled": true,
                "wording": "Report Ad",
                "position": "fixed-bottom-right"
              }
            });
            </script>
        </div>
      </div>
      <aside>
        <div class="small-info">
          <div class="views">
            <span class="text">Views</span><span class="num"></span>
          </div>
        </div>
        <!-- <a href="/images/KaaHPVn.png/download" class="download button" data-track="Download">Download image</a> -->
        
        <button class="report" data-track="Report" data-image="KaaHPVn.png">Report image</button>
        
        <a href="/" class="button confirm" data-track="Home">Upload an image</a>
        <div class="views hide-small">
          <span class="text">Views</span><span class="num"></span>
        </div>
        <div class="gad-container">
          <div class="gad" id="image-side" style="width: 300px; max-height: 250px;"></div>
          <script type="text/javascript">
          nads.createAd('image-side', {
            "refreshLimit": 10,
            "refreshTime": 30,
            "report": {
              "enabled": true,
              "wording": "Report Ad",
              "position": "fixed-bottom-right"
            }
          });
          </script>
      </div>
  </div>
      </aside>
      <footer>
  <div class="box box-left">
    <h1 class="title"><a href="/"><strong>Pasteboard</strong><span class="small">Easy Image Uploads</span></a></h1>
  </div>
  <div class="box box-right">
    <span data-ccpa-link="1" class="ccpa"></span>
    <a href="/premium?utm_source=pasteboard&utm_medium=footer&utm_campaign=premium" target="_blank">Premium</a>
    <span>&middot;</span>
    <a href="/advertise" class="ad-link">Advertise</a>
    <span>&middot;</span>
    <a href="/terms" class="terms-link">Terms &amp; Policy</a>
  </div>
</footer>
      <div class="footer-video-ad">
        <div id="image-video-slider" style="width: 300px; max-height: 250px;"></div>
        <script type="text/javascript">
        window['nitroAds'].createAd('image-video-slider', {
          "format": "video-ac"
        });
        </script>
      </div>
    <script>
      window.PB_IMAGE = "KaaHPVn.png";
    </script>
